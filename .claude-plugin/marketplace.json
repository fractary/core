    },
    "metadata": {
      "description": "Production-ready core plugins powered by Claude Code agents, commands, skills and hooks.",
      "version": "2.1.3",
      "pluginRoot": "./plugins"
    },
    "plugins": [
      {
        "name": "fractary-repo",
        "source": "./plugins/repo",
        "description": "Universal source control operations across GitHub, GitLab, and Bitbucket with modular handler architecture",
        "version": "2.3.3",
        "author": {
          "name": "The Fractary",
          "url": "https://github.com/fractary"
        },
        "homepage": "https://github.com/fractary/core/tree/main/plugins/repo",
        "repository": "https://github.com/fractary/core/tree/main/plugins/repo",
        "license": "MIT",
        "keywords": [
          "git",
          "github",
          "source-control",
          "repository",
          "version-control",
          "branches",
          "commits",
          "pull-requests"
        ],
        "category": "development",
        "strict": true,
        "commands": [
          "./commands/commit-push-pr.md",
          "./commands/commit-push.md",
          "./commands/commit.md",
          "./commands/init.md",
          "./commands/pr-create.md",
          "./commands/pr-merge.md",
          "./commands/pr-review.md",
          "./commands/pull.md"
        ],
        "agents":  [
          "./agents/init.md"
