{
    "name": "fractary-core",
    "owner": {
      "name": "The Fractary",
      "email": "admin@fractary.com",
      "url": "https://github.com/fractary/"
    },
    "metadata": {
      "description": "Production-ready core plugins powered by Claude Code agents, commands, skills and hooks.",
      "version": "2.0.2",
      "pluginRoot": "./plugins"
    },
    "plugins": [
      {
        "name": "fractary-repo",
        "source": "./plugins/repo",
        "description": "Universal source control operations across GitHub, GitLab, and Bitbucket with modular handler architecture",
        "version": "2.2.1",
        "author": {
          "name": "The Fractary",
          "url": "https://github.com/fractary"
        },
        "homepage": "https://github.com/fractary/core/tree/main/plugins/repo",
        "repository": "https://github.com/fractary/core/tree/main/plugins/repo",
        "license": "MIT",
        "keywords": [
          "git",
          "github",
          "gitlab",
          "bitbucket",
          "source-control",
          "repository",
          "version-control",
          "branches",
          "commits",
          "pull-requests",
          "tags"
        ],
        "category": "development",
        "strict": true,
        "commands": [
          "./commands/branch.md",
          "./commands/commit.md",
          "./commands/push.md",
          "./commands/pr.md",
          "./commands/tag.md",
          "./commands/cleanup.md",
          "./commands/init.md"
        ],
        "agents": [
          "./agents/repo-manager.md"
        ],
        "hooks": "./hooks/hooks.json"
      },
      {
        "name": "fractary-work",
