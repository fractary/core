        "category": "development",
        "strict": true,
        "commands": [
          "./commands/init.md",
          "./commands/issue-create.md",
          "./commands/issue-fetch.md",
          "./commands/issue-list.md",
          "./commands/issue-search.md",
          "./commands/issue-update.md"
